    "#returns the page views of mod\n",
    "def get_page_views(mod):\n",
    "    try:\n",
    "        page = requests.get(mod.url)\n",
    "    except:\n",
    "        return ''\n",
    "    \n",
    "    soup = BeautifulSoup(page.text, 'html5lib')\n",
    "    return soup.find('p', class_='file-total-views').find('strong').text"
